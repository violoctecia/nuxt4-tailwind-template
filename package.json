{
	"name": "nuxt-app",
	"type": "module",
	"private": true,
	"packageManager": "pnpm@10.11.0",
	"scripts": {
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"prepare": "husky",
		"build": "nuxt build",
		"dev": "nuxt dev",
		"start": "node .output/server/index.mjs",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier --write ."
	},
	"lint-staged": {
		"*.{vue,js,ts,mjs,css,scss}": [
			"npm run prettier",
			"npm run lint:fix"
		]
	},
	"dependencies": {
		"@pinia/nuxt": "0.11.2",
		"nuxt": "^4.0.3",
		"pinia": "^3.0.3",
		"vue": "^3.5.20",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@nuxtjs/tailwindcss": "6.14.0",
		"@nuxt/eslint": "^1.9.0",
		"husky": "^9.1.7",
		"eslint": "^9.34.0",
		"lint-staged": "^16.1.6",
		"eslint-plugin-prettier": "^5.5.4",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.2.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"sass": "^1.92.1",
		"typescript": "^5.9.2",
		"vue-tsc": "^3.0.6"
	}
}
