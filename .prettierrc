{
	"semi": true,
	"singleQuote": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"printWidth": 140,
	"useTabs": true,
	"tabWidth": 4,
	"endOfLine": "auto",
	"arrowParens": "always",
	"embeddedLanguageFormatting": "auto",
	"plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
	"htmlWhitespaceSensitivity": "ignore"
}
